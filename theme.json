{
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultPalette": false,
			"custom": false,
			"duotone": [
				{
					"colors": [ "#000000", "#399e1a" ],
					"slug": "black-and-primary",
					"name": "Black and primary"
				}
			],
			"customDuotone": false,
			"gradients": [],
			"customGradient": false,
			"link": true,
			"palette": [
				{
					"name": "Black",
					"slug": "black",
					"color": "#000000"
				},
				{
					"name": "White",
					"slug": "white",
					"color": "#FFFFFF"
				},
				{
					"name": "Light gray",
					"slug": "lightgray",
					"color": "#EEEEEE"
				},
				{
					"name": "Primary",
					"slug": "primary",
					"color": "#0000FF"
				},
				{
					"name": "Secondary",
					"slug": "secondary",
					"color": "#00FF00"
				},
				{
					"name": "Tertiary",
					"slug": "tertiary",
					"color": "#FF0000"
				}
			]
		},
		"layout": {
			"contentSize": "64rem",
			"wideSize": "80rem"
		},
		"spacing": {
			"customMargin": true,
			"customPadding": true,
			"units": [
				"%",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"dropCap": false,
			"customFontSize": false,
			"customLineHeight": false,
			"fontFamilies": [
				{
					"fontFamily": "Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFamily": "\"Roboto Slab\", serif",
					"name": "Heading Font",
					"slug": "heading-font"
				}
			],
			"fontSizes": [
				{
					"size": "0.75rem",
					"slug": "small"
				},
				{
					"size": "1rem",
					"slug": "normal"
				},
				{
					"size": "1.5rem",
					"slug": "medium"
				},
				{
					"size": "2rem",
					"slug": "large"
				},
				{
					"size": "3rem",
					"slug": "x-large"
				}

			]
		},
		"custom": {
			"typography": {
				"font-size": {
					"gigantic": "clamp(4rem, 6vw, 5rem)",
					"huge": "clamp(4rem, 4vw, 3.75rem)"
				},
				"line-height": {
					"tiny": 1.15,
					"small": 1.2,
					"medium": 1.4,
					"normal": 1.556
				}
			}
		},
		"blocks": {
			"core/button": {
				"color": {
					"palette": [
						{
							"name": "White",
							"slug": "white",
							"color": "#FFFFFF"
						},
						{
							"name": "Primary",
							"slug": "primary",
							"color": "#0000FF"
						}
					]
				}
			}
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--lightergray)",
			"text": "var(--wp--preset--color--black)"
		},
		"typography": {
			"fontSize": "var(--wp--preset--font-size--normal)",
			"fontFamily": "var(--wp--preset--font-family--system-font)",
			"lineHeight": "1.5" 
		},
		"elements": {
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading-font)",
					"fontSize": "var(--wp--custom--typography--font-size--gigantic)",
					"lineHeight": "var(--wp--custom--typography--line-height--small)"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading-font)",
					"fontSize": "var(--wp--custom--typography--font-size--huge)",
					"lineHeight": "var(--wp--custom--typography--line-height--small)"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading-font)",
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading-font)",
					"fontSize": "var(--wp--preset--font-size--large)",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)",
					"fontWeight": 700
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)",
					"fontWeight": 700
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			}
		},
		"blocks": {
			"core/button": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading-font)"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--white)"
				},
				"border": {
					"radius": "0"
				}
			},
			"core/cover": {
				"color": {
					"text": "var(--wp--preset--color--white)"
				}
			}
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		}
	]
}