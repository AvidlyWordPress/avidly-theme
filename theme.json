					},
					"size": "2rem",
					"slug": "huge",
					"name": "Heading 2"
				},
				{
					"fluid": {
						"min": "2.375rem",
						"max": "3.375rem"
					},
					"size": "3.375rem",
					"slug": "gigantic",
					"name": "Heading 1"
				}

			]
		},
		"custom": {
			"typography": {
				"line-height": {
					"tiny": 1.1,
					"small": 1.25,
					"normal": 1.5,
					"medium": 1.65
				}
			},
			"color": {
				"link": "#36597D",
				"warning-dark": "#931B1B",
				"warning-light": "#F8E2E2",
				"success-dark": "#0F6115",
				"success-light": "#E8FCE8"
			  }
		},
		"blocks": {
			"core/button": {
				"color": {
					"palette": [
						{
							"name": "White",
							"slug": "white",
							"color": "#FFFFFF"
						},
						{
							"name": "Black",
							"slug": "black",
							"color": "#2A2A2A"
						},
						{
							"name": "Secondary",
							"slug": "secondary",
							"color": "#AEBECB"
						}
					]
				},
				"typography": {
					"fluid": false,
					"fontSizes": [
						{
							"size": "0.875",
							"slug": "small",
							"name": "Small"
						},
						{
							"size": "1.125rem",
							"slug": "medium",
							"name": "Normal"
						},
						{
							"size": "1.5rem",
							"slug": "xlarge",
							"name": "xLarge"
						}
					]
				}
			}
		}
	},
	"styles": {
		"spacing": {
			"blockGap": "1.5rem"
		},
		"color": {
			"background": "var(--wp--preset--color--lightergray)",
			"text": "var(--wp--preset--color--black)"
		},
		"typography": {
			"fontSize": "var(--wp--preset--font-size--normal)",
			"fontFamily": "var(--wp--preset--font-family--system-font)",
			"lineHeight": "var(--wp--custom--typography--line-height--normal)"
		},
		"elements": {
			"summary": {
				"typography": {
					"fontWeight": 700
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading-font)",
					"fontWeight": 700,
					"lineHeight": "var(--wp--custom--typography--line-height--small)"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--gigantic)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--huge)"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--system-font)",
					"fontSize": "var(--wp--preset--font-size--xlarge)",
					"fontWeight": 400
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--system-font)",
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--system-font)",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"textTransform": "uppercase"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--system-font)",
					"fontSize": "var(--wp--preset--font-size--normal)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--custom--color--link)"
				},
				"typography": {
					"textDecoration": "underline",
					"fontWeight": 700
				},
				":hover": {
					"typography": {
						"textDecoration": "underline dashed"
					}
				},
				":focus": {
					"typography": {
						"textDecoration": "underline dashed"
					}
				}
			},
			"button": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": 500
				},
				"border": {
					"radius": "5em"
				},
				"color": {
					"background": "var(--wp--preset--color--secondary)",
					"text": "var(--wp--preset--color--black)"
				},
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--xs)",
						"left": "var(--wp--preset--spacing--md)",
						"bottom": "var(--wp--preset--spacing--xs)",
						"right": "var(--wp--preset--spacing--md)"
					}
				},
				":hover": {
					"typography": {
						"textDecoration": "underline"
					}
				},
				":focus": {
					"typography": {
						"textDecoration": "underline dashed"
					}
				}
			}
		},
		"blocks": {
			"core/buttons": {
				"spacing": {
					"blockGap": "1rem"
				}
			},
			"core/cover": {
				"color": {
					"text": "var(--wp--preset--color--white)"
				}
			},
			"core/quote": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"lineHeight": "var(--wp--custom--typography--line-height--medium)"
				}
			},
			"core/columns": {
				"spacing": {
					"blockGap": "2rem"
				}
			},
			"core/post-title": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--black)"
						},
						"typography": {
							"textDecoration": "none"
						},
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				}
			}
		}
	},
	"customTemplates": [
		{
			"name": "block-template",
			"title": "Block template",
			"postTypes": [ "page" ]
		}
	]
}