			"fluid": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontDisplay": "optional",
							"fontFamily": "Roboto",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/webfonts/Roboto/Roboto-Regular.ttf"
							]
						},
						{
							"fontDisplay": "optional",
							"fontFamily": "Roboto",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": [
								"file:./assets/webfonts/Roboto/Roboto-Italic.ttf"
							]
						},
						{
							"fontDisplay": "optional",
							"fontFamily": "Roboto",
							"fontStretch": "normal",
							"fontStyle": "medium",
							"fontWeight": "500",
							"src": [
								"file:./assets/webfonts/Roboto/Roboto-Medium.ttf"
							]
						},
						{
							"fontDisplay": "optional",
							"fontFamily": "Roboto",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/webfonts/Roboto/Roboto-Bold.ttf"
							]
						}
					],
					"fontFamily": "Roboto, Oxygen-Sans, Ubuntu, Cantarell, \"Helvetica Neue\", sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFace": [
						{
							"fontDisplay": "optional",
							"fontFamily": "Roboto Slab",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/webfonts/Roboto_Slab/RobotoSlab-Regular.ttf"
							]
						},
						{
							"fontDisplay": "optional",
							"fontFamily": "Roboto Slab",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/webfonts/Roboto_Slab/RobotoSlab-Bold.ttf"
							]
						}
					],
					"fontFamily": "\"Roboto Slab\", Georgia, \"Times New Roman\", serif",
					"name": "Heading Font",
					"slug": "heading-font"
				}
			],
			"fontSizes": [
				{
					"fluid": false,
					"size": "0.813rem",
					"slug": "small",
					"name": "Small"
				},
				{
					"fluid": false,
					"size": "1rem",
					"slug": "normal",
					"name": "Paragraph"
				},
				{
					"fluid": {
						"min": "1rem",
						"max": "1.125rem"
					},
					"size": "1.125rem",
					"slug": "medium",
					"name": "Ingress"
				},
				{
					"fluid": {
						"min": "1rem",
						"max": "1.25rem"
					},
					"size": "1.25rem",
					"slug": "large",
					"name": "Heading 4"
				},
				{
					"fluid": {
						"min": "1.125rem",
						"max": "1.5rem"
					},
					"size": "1.5rem",
					"slug": "xlarge",
					"name": "Heading 3"
				},
				{
					"fluid": {
						"min": "1.5rem",
