{
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultPalette": false,
			"custom": false,
			"duotone": [
				{
					"colors": [ "#000000", "#399e1a" ],
					"slug": "black-and-primary",
					"name": "Black and primary"
				}
			],
			"customDuotone": false,
			"gradients": [],
			"customGradient": false,
			"link": true,
			"palette": [
				{
					"name": "Black",
					"slug": "black",
					"color": "#000000"
				},
				{
					"name": "White",
					"slug": "white",
					"color": "#FFFFFF"
				},
				{
					"name": "Light gray",
					"slug": "lightgray",
					"color": "#EEEEEE"
				},
				{
					"name": "Primary",
					"slug": "primary",
					"color": "#0000FF"
				},
				{
					"name": "Secondary",
					"slug": "secondary",
					"color": "#00FF00"
				},
				{
					"name": "Tertiary",
					"slug": "tertiary",
					"color": "#FF0000"
				}
			]
		},
		"layout": {
			"contentSize": "64rem",
			"wideSize": "80rem"
		},
		"spacing": {
			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": [
				{
					"size": "clamp(1.5rem, 5vw, 2rem)",
					"slug": "xs",
					"name": "xsmall"
				},
				{
					"size": "clamp(1.8rem, 1.8rem + ((1vw - 0.48rem) * 2.885), 3rem)",
					"slug": "sm",
					"name": "small"
				},
				{
					"size": "clamp(2.5rem, 8vw, 4.5rem)",
					"slug": "md",
					"name": "medium"
				},
				{
					"size": "clamp(3.75rem, 10vw, 7rem)",
					"slug": "lg",
					"name": "large"
				},
				{
					"size": "clamp(5rem, 5.25rem + ((1vw - 0.48rem) * 9.096), 8rem)",
					"slug": "xl",
					"name": "xlarge"
				},
				{
					"size": "clamp(7rem, 14vw, 11rem)",
					"slug": "xxl",
					"name": "xxlarge"
				}
			],
			"customMargin": true,
			"customPadding": true,
			"units": [
				"%",
				"rem",
				"em",
				"px",
				"vh",
				"vw"
			]
		},
		"typography": {
			"dropCap": false,
			"customFontSize": false,
			"customLineHeight": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontDisplay": "optional",
							"fontFamily": "Roboto",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/webfonts/Roboto/Roboto-Regular.ttf"
							]
						},
						{
							"fontDisplay": "optional",
							"fontFamily": "Roboto",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": [
								"file:./assets/webfonts/Roboto/Roboto-Italic.ttf"
							]
						},
						{
							"fontDisplay": "optional",
							"fontFamily": "Roboto",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/webfonts/Roboto/Roboto-Bold.ttf"
							]
						}
					],
					"fontFamily": "Roboto, Oxygen-Sans, Ubuntu, Cantarell, \"Helvetica Neue\", sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFace": [
						{
							"fontDisplay": "optional",
							"fontFamily": "Roboto Slab",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/webfonts/Roboto_Slab/RobotoSlab-Regular.ttf"
							]
						},
						{
							"fontDisplay": "optional",
							"fontFamily": "Roboto Slab",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/webfonts/Roboto_Slab/RobotoSlab-Bold.ttf"
							]
						}
					],
					"fontFamily": "\"Roboto Slab\", Georgia, \"Times New Roman\", serif",
					"name": "Heading Font",
					"slug": "heading-font"
				}
			],
			"fontSizes": [
				{
					"fluid": {
						"min": "0.85rem",
						"max": "0.9rem"
					},
					"size": "0.9rem",
					"slug": "small",
					"name": "Small"
				},
				{
					"fluid": {
						"min": "1rem",
						"max": "1.15rem"
					},
					"size": "1.15rem",
					"slug": "normal",
					"name": "Paragraph"
				},
				{
					"fluid": {
						"min": "1.25rem",
						"max": "1.5rem"
					},
					"size": "1.5rem",
					"slug": "medium",
					"name": "Ingress"
				},
				{
					"fluid": {
						"min": "1.5rem",
						"max": "2rem"
					},
					"size": "2rem",
					"slug": "large",
					"name": "Heading 4"
				},
				{
					"fluid": {
						"min": "2rem",
						"max": "2.5rem"
					},
					"size": "2.5rem",
					"slug": "xlarge",
					"name": "Heading 3"
				},
				{
					"fluid": {
						"min": "2.35rem",
						"max": "3.25rem"
					},
					"size": "3.25rem",
					"slug": "huge",
					"name": "Heading 2"
				},
				{
					"fluid": {
						"min": "2.75rem",
						"max": "4.5rem"
					},
					"size": "4.5rem",
					"slug": "gigantic",
					"name": "Heading 1"
				}

			]
		},
		"custom": {
			"typography": {
				"line-height": {
					"tiny": 1.1,
					"small": 1.25,
					"normal": 1.556,
					"medium": 1.65
				}
			}
		},
		"blocks": {
			"core/button": {
				"color": {
					"palette": [
						{
							"name": "White",
							"slug": "white",
							"color": "#FFFFFF"
						},
						{
							"name": "Primary",
							"slug": "primary",
							"color": "#0000FF"
						}
					]
				}
			}
		}
	},
	"styles": {
		"spacing": {
			"blockGap": "1.5rem"
		},
		"color": {
			"background": "var(--wp--preset--color--lightergray)",
			"text": "var(--wp--preset--color--black)"
		},
		"typography": {
			"fontSize": "var(--wp--preset--font-size--normal)",
			"fontFamily": "var(--wp--preset--font-family--system-font)",
			"lineHeight": "var(--wp--custom--typography--line-height--normal)"
		},
		"elements": {
			
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading-font)",
					"fontWeight": 700,
					"lineHeight": "var(--wp--custom--typography--line-height--small)"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--gigantic)",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)",
					"fontWeight": 700
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--huge)",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xlarge)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				"typography": {
					"textDecoration": "underline"
				},
				":hover": {
					"typography": {
						"textDecoration": "underline dashed"
					}
				},
				":focus": {
					"typography": {
						"textDecoration": "underline dashed"
					}
				}
			},
			"button": {
				"border": {
					"radius": "0"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--white)"
				},
				":hover": {
					"typography": {
						"textDecoration": "underline"
					}
				},
				":focus": {
					"typography": {
						"textDecoration": "underline dashed"
					}
				}
			}
		},
		"blocks": {
			"core/buttons": {
				"spacing": {
					"blockGap": "1rem"
				}
			},
			"core/cover": {
				"color": {
					"text": "var(--wp--preset--color--white)"
				}
			},
			"core/quote": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"lineHeight": "var(--wp--custom--typography--line-height--medium)"
				}
			},
			"core/columns": {
				"spacing": {
					"blockGap": "2rem"
				}
			},
			"core/post-title": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--black)"
						},
						"typography": {
							"textDecoration": "none"
						},
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				}
			}
		}
	},
	"customTemplates": [
		{
			"name": "block-template",
			"title": "Block template",
			"postTypes": [ "page" ]
		}
	]
}